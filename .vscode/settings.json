// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "client/**/*.js": {
      "when": "$(basename).tsx"
    },
    "{client,server}/**/*.js": {
      "when": "$(basename).ts"
    }
  }
}